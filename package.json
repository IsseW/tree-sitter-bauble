{
  "name": "tree-sitter-bauble",
  "version": "0.1.0",
  "description": "A dependency driven asset decleration language",
  "repository": "github:tree-sitter/tree-sitter-bauble",
  "license": "MIT",
  "author": {
    "name": "Isse",
    "email": "git@isse.rs"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "bauble"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.0.0",
    "node-gyp-build": "^4.8.1"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.24.1"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  },
  "tree-sitter": [
    {
      "scope": "source.bauble",
      "file-types": ["bbl"],
      "injection-regex": "^bauble$",
      "highlights": [
        "queries/highlights.scm"
      ],
      "locals": [
        "queries/locals.scm"
      ]
    }
  ]
}
